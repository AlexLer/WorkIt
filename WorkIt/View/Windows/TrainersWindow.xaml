<Window x:Class="WorkIt.View.Windows.TrainersWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TrainersWindow" Height="200" Width="200">
    <DockPanel>
        <DockPanel.Background>
            <ImageBrush ImageSource="Capture.JPG"/>
        </DockPanel.Background>
        <StackPanel>
            <Button DockPanel.Dock="Top" Content="Add Trainer" Width="100" Height="40" Margin="3"/>
            <Button DockPanel.Dock="Top" Content="Delete Trainer" Width="100" Height="40" Margin="3"/>
            <Button DockPanel.Dock="Top" Content="Update profile" Width="100" Height="40" Margin="3"/>
        </StackPanel>
    </DockPanel>
</Window>
