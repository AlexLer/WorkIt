<Window x:Class="WorkIt.View.Windows.MembersWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MembersWindow" Height="300" Width="300">
    <DockPanel>
        <DockPanel.Background>
            <ImageBrush ImageSource="Capture.JPG"/>
        </DockPanel.Background>
        <StackPanel DockPanel.Dock="Left">
            <Button Content="New Memeber" Width="130" Margin="3" Click="addMember_btn_click"/>
            <Button Content="Update Profile" Width="130" Margin="3"/>
            <Button Content="Delete Memeber" Width="130" Margin="3"/>
            <Button Content="Rewnew Memebership" Width="130" Margin="3"/>
            <Button Content="Sign for class" Width="130" Margin="3"/>
        </StackPanel>
        
    </DockPanel>
</Window>
